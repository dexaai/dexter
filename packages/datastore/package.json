{
  "name": "@dexaai/datastore",
  "type": "module",
  "version": "0.4.1",
  "description": "",
  "repository": "dexaai/llm-tools",
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.dist.json",
    "clean": "rm -rf .turbo node_modules dist",
    "dev": "tsc --watch",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./pinecone": {
      "types": "./dist/pinecone/index.d.ts",
      "import": "./dist/pinecone/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "dependencies": {
    "@dexaai/model": "workspace:*",
    "@fastify/deepmerge": "^1.3.0",
    "pinecone-client": "^1.1.0"
  },
  "devDependencies": {
    "@dexaai/eslint-config": "^0.4.0",
    "@types/node": "^20.5.9",
    "eslint": "^8.48.0",
    "tsconfig": "workspace:*",
    "type-fest": "4.3.1",
    "typescript": "^5.2.2",
    "vite": "^4.3.9",
    "vitest": "^0.34.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
