{
  "name": "@dexaai/ai",
  "type": "module",
  "version": "0.0.1",
  "description": "",
  "repository": "dexaai/llm-tools",
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.dist.json",
    "clean": "rm -rf node_modules dist",
    "dev": "tsc --watch",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "prepare": "npm run build",
    "release": "np",
    "test": "dotenv -e .env.example -- vitest",
    "typecheck": "tsc --noEmit"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./datastore": {
      "types": "./dist/datastore/index.d.ts",
      "import": "./dist/datastore/index.js"
    },
    "./datastore/pinecone": {
      "types": "./dist/datastore/pinecone/index.d.ts",
      "import": "./dist/datastore/pinecone/index.js"
    },
    "./prompt": {
      "types": "./dist/prompt/index.d.ts",
      "import": "./dist/prompt/index.js"
    },
    "./model": {
      "types": "./dist/model/index.d.ts",
      "import": "./dist/model/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "dependencies": {
    "@fastify/deepmerge": "^1.3.0",
    "dedent": "^1.5.1",
    "jsonrepair": "^3.2.0",
    "ky": "^1.1.0",
    "openai": "^4.14.1",
    "openai-fetch": "2.0.0-beta.6",
    "p-map": "^6.0.0",
    "p-throttle": "^5.1.0",
    "parse-json": "^7.0.0",
    "pinecone-client": "^1.1.0",
    "tiktoken": "^1.0.10",
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.21.4",
    "zod-validation-error": "^1.3.1"
  },
  "devDependencies": {
    "@dexaai/eslint-config": "^0.4.0",
    "@types/node": "^20.5.9",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.48.0",
    "np": "^8.0.4",
    "type-fest": "4.3.1",
    "typescript": "^5.2.2",
    "vite": "^4.3.9",
    "vitest": "^0.34.3"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "engines": {
    "node": ">= 18"
  }
}
