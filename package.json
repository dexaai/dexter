{
  "name": "@dexaai/dexter",
  "type": "module",
  "version": "3.0.0-3",
  "description": "LLM tools used in production at Dexa, with a focus on real-world RAG.",
  "repository": "dexaai/dexter",
  "license": "MIT",
  "author": "Riley Tomasek <riley@dexa.ai>",
  "contributors": [
    "Riley Tomasek <riley@dexa.ai>",
    "Travis Fischer <travis@transitivebullsh.it>"
  ],
  "engines": {
    "node": ">= 18"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/model/index.d.ts",
      "import": "./dist/model/index.js"
    },
    "./ai-function": {
      "types": "./dist/ai-function/index.d.ts",
      "import": "./dist/ai-function/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.dist.json",
    "clean": "rimraf node_modules dist",
    "dev": "tsc --watch",
    "fix": "eslint --fix --quiet . ; prettier --write --log-level=silent .",
    "format": "prettier --check \"**/*.{js,ts,tsx}\"",
    "knip": "knip",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "prebuild": "rimraf dist",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm run lint && pnpm run typecheck",
    "release": "np --no-cleanup",
    "test": "dotenv -e .env.example -- vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@fastify/deepmerge": "^2.0.0",
    "dedent": "^1.5.3",
    "hash-object": "^5.0.1",
    "jsonrepair": "^3.8.1",
    "ky": "^1.7.2",
    "openai-fetch": "3.3.1",
    "p-map": "^7.0.2",
    "p-throttle": "^6.2.0",
    "parse-json": "^8.1.0",
    "tiktoken": "^1.0.17",
    "tslib": "^2.7.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.3",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@dexaai/eslint-config": "^1.3.6",
    "@sentry/node": "^8.34.0",
    "@types/node": "^20.14.11",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "knip": "^5.33.3",
    "np": "^10.0.7",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "type-fest": "4.26.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@9.12.1"
}
